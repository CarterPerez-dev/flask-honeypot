        function closePopup(id) {
            const popup = document.getElementById(id);
            if (popup) {
                // Remove from DOM
                document.body.removeChild(popup);
                
                // Remove from array
                popups = popups.filter(p => p !== id);
                
                // Log the interaction
                logInteraction('popup_closed', { id });
            }
        }
        
